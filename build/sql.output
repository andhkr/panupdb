Grammar

    0 $accept: query $end

    1 query: ε
    2      | query sql_statement

    3 sql_statement: select_statement ';'
    4              | insert_statement ';'
    5              | update_statement
    6              | delete_statement
    7              | create_statement ';'
    8              | drop_table ';'

    9 select_statement: SELECT select_list FROM table_reference_list where_clause_opt group_by_clause_opt having_clause_opt order_by_clause_opt limit_clause_opt offset_clause_opt

   10 select_list: select_item
   11            | select_list ',' select_item

   12 select_item: expression
   13            | expression AS IDENTIFIER
   14            | expression IDENTIFIER

   15 table_reference_list: table_reference

   16 table_reference: table_name
   17                | table_name IDENTIFIER
   18                | table_name AS IDENTIFIER
   19                | table_reference join_type JOIN table_reference ON search_condition

   20 table_name: IDENTIFIER

   21 join_type: ε
   22          | INNER
   23          | LEFT OUTER
   24          | RIGHT OUTER
   25          | FULL OUTER
   26          | LEFT
   27          | RIGHT
   28          | FULL

   29 where_clause_opt: ε
   30                 | WHERE search_condition

   31 group_by_clause_opt: ε
   32                    | GROUP BY column_reference_list

   33 column_reference_list: column_reference
   34                      | column_reference_list ',' column_reference

   35 column_reference: IDENTIFIER
   36                 | table_name '.' IDENTIFIER

   37 having_clause_opt: ε
   38                  | HAVING search_condition

   39 order_by_clause_opt: ε
   40                    | ORDER BY order_by_list

   41 order_by_list: order_by_item
   42              | order_by_list ',' order_by_item

   43 order_by_item: expression
   44              | expression ASC
   45              | expression DESC

   46 limit_clause_opt: ε
   47                 | LIMIT expression

   48 offset_clause_opt: ε
   49                  | OFFSET expression

   50 insert_statement: INSERT INTO table_name insert_columns_clause VALUES '(' insert_values_list ')'

   51 insert_columns_clause: ε
   52                      | '(' column_reference_list ')'

   53 insert_values_list: expression
   54                   | insert_values_list ',' expression

   55 update_statement: UPDATE table_name SET update_list where_clause_opt

   56 update_list: update_item
   57            | update_list ',' update_item

   58 update_item: column_reference '=' expression

   59 delete_statement: DELETE FROM table_name where_clause_opt

   60 create_statement: CREATE TABLE table_name '(' column_definition_list ')'

   61 column_definition_list: column_definition
   62                       | column_definition_list ',' column_definition

   63 column_definition: IDENTIFIER data_type column_constraint_list_opt

   64 data_type: INT
   65          | CHAR '(' INTEGER_LITERAL ')'
   66          | VARCHAR '(' INTEGER_LITERAL ')'
   67          | NUMBER
   68          | DATE
   69          | TEXT

   70 column_constraint_list_opt: ε
   71                           | column_constraint_list

   72 column_constraint_list: column_constraint
   73                       | column_constraint_list column_constraint

   74 column_constraint: NOT SQLNULL
   75                  | SQLNULL
   76                  | PRIMARY KEY
   77                  | UNIQUE
   78                  | DEFAULT default_value
   79                  | FOREIGN KEY REFERENCES table_name '(' IDENTIFIER ')'

   80 default_value: INTEGER_LITERAL
   81              | STRING_LITERAL

   82 drop_table: DROP table_name

   83 search_condition: search_condition OR search_condition
   84                 | search_condition AND search_condition
   85                 | NOT search_condition
   86                 | '(' search_condition ')'
   87                 | predicate

   88 predicate: comparison_predicate
   89          | between_predicate
   90          | in_predicate
   91          | like_predicate
   92          | null_predicate
   93          | exists_predicate

   94 comparison_predicate: expression '=' expression
   95                     | expression '<' expression
   96                     | expression '>' expression
   97                     | expression LE expression
   98                     | expression GE expression
   99                     | expression NE expression

  100 between_predicate: expression BETWEEN expression AND expression
  101                  | expression NOT BETWEEN expression AND expression

  102 in_predicate: expression IN '(' expression_list ')'
  103             | expression NOT IN '(' expression_list ')'

  104 like_predicate: expression LIKE expression
  105               | expression NOT LIKE expression

  106 null_predicate: expression IS SQLNULL
  107               | expression IS NOT SQLNULL

  108 exists_predicate: EXISTS '(' select_statement ')'

  109 expression_list: expression
  110                | expression_list ',' expression

  111 expression: term
  112           | expression '+' expression
  113           | expression '-' expression
  114           | expression '*' expression
  115           | expression '/' expression
  116           | '-' expression
  117           | '(' expression ')'
  118           | function_call

  119 term: IDENTIFIER
  120     | table_name '.' IDENTIFIER
  121     | INTEGER_LITERAL
  122     | FLOAT_LITERAL
  123     | STRING_LITERAL
  124     | '*'

  125 function_call: IDENTIFIER '(' ')'
  126              | IDENTIFIER '(' function_arg_list ')'

  127 function_arg_list: expression
  128                  | function_arg_list ',' expression


